{
  "name": "vector-cache-proxy",
  "version": "1.0.0",
  "description": "A vector-based semantic caching library with Redis for LLM responses",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run build:copy",
    "build:copy": "cp package.json dist/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vector",
    "cache",
    "redis",
    "llm",
    "semantic-search",
    "embedding",
    "ai",
    "proxy"
  ],
  "author": "Dang Huynh <danghuynh.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/haidang1810/vector-cache-proxy"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "ioredis": "^5.3.2"
  }
}
